{
  "name": "strapi-mapper",
  "version": "1.0.0",
  "description": "A nice greeter",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "test": "nodemon --ext ts --exec 'jasmine-ts  --config=./jasmine.json'",
    "coverage": "nyc npm run test",
    "build": "tsc",
    "format": "prettier --write \"src/**/*.ts\" \"src/**/*.js\"",
    "lint": "tslint -p tsconfig.json",
    "prepublishOnly": "npm test && npm run lint",
    "preversion": "npm run lint",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags",
    "t": "node ./query.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rahff/Strapi-mapper.git"
  },
  "keywords": [
    "Strapi",
    "mapper"
  ],
  "author": "Rahff",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/rahff/Strapi-mapper/issues"
  },
  "homepage": "https://github.com/rahff/Strapi-mapper#readme",
  "devDependencies": {
    "@types/jasmine": "^3.3.12",
    "@types/node": "^18.0.0",
    "@types/qs": "^6.9.7",
    "jasmine": "^3.4.0",
    "jasmine-ts": "^0.3.0",
    "jasmine-ts-console-reporter": "^3.1.1",
    "nodemon": "^1.18.11",
    "nyc": "^15.1.0",
    "prettier": "^2.7.1",
    "ts-node": "^8.0.3",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^4.7.4"
  },
  "files": [
    "lib/**/*"
  ],
  "dependencies": {
    "axios": "^0.27.2",
    "qs": "^6.10.5"
  }
}
